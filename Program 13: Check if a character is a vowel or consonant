#include <stdio.h>

void main() {
    char ch;

    printf("Enter a charater: ");

    scanf("%c", &ch);
     if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U'){
        printf("the chatater is vowel.\n");
     }else{
        printf("the chatater is a consonant.\n");
     }

}
